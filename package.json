{
  "name": "fform-flutter",
  "displayName": "FForm Flutter",
  "publisher": "Contributor",
  "description": "FForm Flutter is a Visual Studio Code extension that helps you create forms and fields for Flutter applications.",
  "version": "0.0.2",
  "repository": "https://github.com/contributors-company",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Extension Packs"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fform-flutter.createForm",
        "title": "FForm: Create Form",
        "when": false
      },
      {
        "command": "fform-flutter.createField",
        "title": "FForm: Create Field",
        "when": false
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "fform-flutter.createForm",
          "when": "explorerResourceIsFolder",
          "group": "fform"
        },
        {
          "command": "fform-flutter.createField",
          "when": "explorerResourceIsFolder",
          "group": "fform"
        }
      ]
    },
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/dart.json"
      }
    ],
    "configuration": {
      "title": "My Extension Settings",
      "properties": {
        "fform-flutter.showCommands": {
          "type": "boolean",
          "default": true,
          "description": "Controls whether fform-flutter commands are shown in command palette and other places."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  }
}
